# - Config file for the zetton_common package
# It defines the following variable
# zetton_common_LIBRARIES - libraries to link against

include(CMakeFindDependencyMacro)

# Find dependencies
find_dependency(Threads REQUIRED)
find_dependency(OpenCV 4 REQUIRED)
find_dependency(fmt REQUIRED)
find_dependency(absl REQUIRED)

# Our library dependencies (contains definitions for IMPORTED targets)
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# These are IMPORTED targets created by @PROJECT_NAME@Targets.cmake
set(zetton_common_LIBRARIES @PROJECT_NAME@::@PROJECT_NAME@)
